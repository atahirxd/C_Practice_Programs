#include<stdio.h>
void main()
{
	int num, num1, num2, num3;
	num = 100;
	while (num < 1000)
	{
		num1 = num / 100;
		num2 = num % 100 / 10;
		num3 = num % 10;
		if (num1 * num1 * num1 + num2 * num2 * num2 + num3 * num3 * num3 == num)
		{
			printf("%d is an Armstrong number.\n", num);
		}
		num += 1;
	}
}
