#include<stdio.h>
void main()
{
	int arr[10], i, sum = 0;
	double average;
	printf("Enter 10 numbers to be entered: ");
	for (i = 0; i < 10; i++)
		scanf_s("%d", &arr[i]);
	for (i = 0; i < 10; i++)
	{
		sum = sum + arr[i];
	}
	average = (double)sum / 10;
	printf("The average is: %.2lf", average);
}
