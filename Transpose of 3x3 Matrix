#include<stdio.h>
#define SIZE 3
void main()
{
	int arr[SIZE][SIZE];
	printf("Enter values for the 3x3 matrix: ");
	for (int i = 0; i < SIZE; i++)
		for (int j = 0; j < SIZE; j++)
			scanf_s("%d", &arr[i][j]);
	printf("The original matrix is: ");
	for (int i = 0; i < SIZE; i++)
	{
		printf("\n");
		for (int j = 0; j < SIZE; j++)
			printf("%d\t", arr[i][j]);

	}
	printf("\nThe transpose matrix is: ");
	for (int i = 0; i < SIZE; i++)
	{
		printf("\n");
		for (int j = 0; j < SIZE; j++)
			printf("%d\t", arr[j][i]);
	}
}
