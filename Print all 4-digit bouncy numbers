#include <stdio.h>
void main()
{
	int counter_num, num1, num2, num3, num4, bouncy;
	counter_num = 1000;
	do {
		num1 = counter_num / 1000;
		num2 = counter_num % 1000 / 100;
		num3 = counter_num % 1000 % 100 / 10;
		num4 = counter_num % 10;
		if ((num1 == num2) && (num1 == num3) && (num1 == num4))
		counter_num!=bouncy;
		else if ((num2 >= num1) && (num3 >= num2) && (num4 >= num3))
		counter_num!=bouncy;
		else if ((num2 <= num1) && (num3 <= num2) && (num4 <= num3))
		counter_num != bouncy;
		else
		{
		bouncy=counter_num;
		printf("%d\t\t", bouncy);
		}
		counter_num++;
	} while (counter_num>999 && counter_num<10000);
	
}
